{
  "name": "brf-marketing-design-template",
  "version": "1.0.0",
  "description": "Marketing Design Template",
  "author": "Brian Francisco",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
